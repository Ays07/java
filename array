package ayush;

class array {

	public static void main(String[] args) {
		int week_days[];
		week_days=new int[7];
		week_days[0]=1;
		week_days[1]=2;
		week_days[2]=3;
		week_days[3]=4;
		week_days[4]=5;
		week_days[5]=6;
		week_days[6]=7;
		
		System.out.println("tuesday has"+week_days[2]+"days");

	}

}
